{
    "extends": "airbnb-base",
    "rules": {
      "semi": ["warn", "never"],
      "no-use-before-define": ["error", { "functions": false, "classes": true, "variables": true }],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "never"
        }
      ],
      "no-plusplus": 0
    }
}
